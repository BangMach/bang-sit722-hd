# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.22.1

# Language settings
language-settings:
  javascript: true

# Patches applied by Snyk
patches: {}

# Ignore certain vulnerabilities (use carefully and with time limits)
ignore: {}

# Exclude paths from scanning
exclude:
  global:
    - node_modules/**
    - tests/**
    - coverage/**
    - dist/**
    - build/**